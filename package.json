{
  "name": "ableton-rack-analyzer",
  "version": "1.0.0",
  "description": "Ableton Rack Analyzer with React Frontend",
  "scripts": {
    "build": "./build.sh",
    "frontend:install": "cd frontend-new && npm ci",
    "frontend:build": "cd frontend-new && npm run build",
    "backend:start": "cd backend && python -m gunicorn app:app --bind 0.0.0.0:$PORT --timeout 120"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}